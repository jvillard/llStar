/* %y = load  %t* %x */
procedure llvm_load(i64 %x, lltype %t, llany _v) returns (llany %y)
  {pointer(i64 %x, lltype %t, as(llany _v))}
  {pointer(i64 %x, lltype %t, as(llany _v)) * llany %y = llany _v }
;

/* store %t %v %t* %x */
procedure llvm_store(i64 %x, lltype %t, llany %v)
  {pointer(i64 %x, lltype %t, llmem _v)}
  {pointer(i64 %x, lltype %t, as(llany %v))}
;
