/* test if implications hold with this empty function */
skip:
   {!GT(x, y)}
   {!GT(builtin_plus(x,numeric_const("12")), y)}

main:
   {}
   {$ret_v1 = builtin_bvmul(bv_const("32","32"),bv_const("32", "52"))}

/* this spec is equivalent to f: {}{$ret_v1 = @parameter0:} */
f:
   {@parameter0: = _x}
   {$ret_v1 = _x}
