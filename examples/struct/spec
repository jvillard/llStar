  {!type(a,bv_const("32", "0")) * !type(b,bv_const("64", "1"))}
  {pointer($ret_v1,sizeof(named_type("node")),concat.64.64(@parameter1:,concat.32.32(_pad,@parameter0:))) * malloced($ret_v1,sizeof(named_type("node")))
   || $ret_v1 = NULL()}

chew_slowly:
